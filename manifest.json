{
  "manifest_version": 3,
  "name": "Freelance Assist",
  "version": "1.0",
  "description": "Quick access to Freelance Assist platform",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["activeTab"],
  "host_permissions": ["https://freelanceassists.netlify.app/*"]
}